<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextView">
        <attr name="fontName" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="fontName" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="fontName" />
    </declare-styleable>


    <attr name="fontName" format="enum">
        <enum name="ProximaNova_Bold" value="1" />
        <enum name="ProximaNova_Extrabld" value="2" />
        <enum name="ProximaNova_Light" value="3" />
        <enum name="ProximaNova_LightIt" value="4" />
        <enum name="ProximaNova_Regular" value="5" />
        <enum name="ProximaNova_RegularIt" value="6" />
        <enum name="ProximaNova_Semibold" value="7" />
        <enum name="ProximaNova_SemiboldIt" value="8" />
        <enum name="GOT" value="9" />
    </attr>


</resources>